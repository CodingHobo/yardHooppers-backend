{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "price": {
      "type": "string",
      "pattern": "([0-9])"

    },
    "host_user": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "city": {
      "type": "string",
      "maxLength": 30
    },
    "state": {
      "type": "string",
      "maxLength": 30
    },
    "zipcode": {
      "type": "string",
      "pattern": "0|(0?\\.[0-9]+)",
      "maxLength": 5
    },
    "address": {
      "type": "string",
      "maxLength": 25
    },
    "description": {
      "type": "string",
      "minLength": 10
    },
    "photo_url": {
      "type": "string",
      "maxLength": 500,
      "format": "uri"
    }
  },
  "additionalProperties": false,
  "required": [
    "price",
    "host_user",
    "city",
    "zipcode",
    "address",
    "description",
    "state"
  ]
}
